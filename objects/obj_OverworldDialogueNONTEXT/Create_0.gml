show = true;
relativeX = 273
relativeY = 73
_x = x
_y = y
relXCalc = function(num) {
	var DIF = relativeX - num
	return relativeX - DIF + _x
}
relYCalc = function(num) {
	var DIF = relativeY - num
	return relativeY - DIF + _y
}


